{
  "name": "sample",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3333",
    "build": "next build --debug",
    "start": "node server.js -p 3333 -H 0.0.0.0",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && pnpm format",
    "lint:strict": "eslint --max-warnings=1000 src",
    "typecheck": "tsc --noEmit --incremental false",
    "commitNow": "git add . && git commit -m Update && git push",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@next/font": "^14.2.15",
    "@next/third-parties": "^14.2.5",
    "@nextui-org/accordion": "^2.0.40",
    "@nextui-org/modal": "^2.0.41",
    "@nextui-org/react": "^2.4.8",
    "@nextui-org/slider": "^2.2.17",
    "@nextui-org/system": "^2.2.6",
    "@nextui-org/theme": "^2.2.11",
    "@types/qs": "^6.9.11",
    "@types/react-google-recaptcha": "^2.1.9",
    "axios": "^1.6.7",
    "cheerio": "1.0.0-rc.12",
    "clsx": "^2.0.0",
    "dotenv": "^16.4.5",
    "file-loader": "^6.2.0",
    "flat": "^6.0.1",
    "framer-motion": "^11.9.0",
    "lottie-react": "^2.4.0",
    "moment": "^2.30.1",
    "next": "^14.2.0",
    "qs": "^6.11.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-google-recaptcha": "^3.1.0",
    "react-hook-form": "^7.50.0",
    "react-icons": "^4.12.0",
    "sharp": "^0.33.3",
    "swiper": "^11.0.5",
    "tailwind-merge": "^2.1.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.3.0",
    "@commitlint/config-conventional": "^16.2.4",
    "@next/bundle-analyzer": "^14.1.0",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/forms": "^0.5.7",
    "@types/next-auth": "^3.15.0",
    "@types/react": "^18.2.45",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.2.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.5.0",
    "next-router-mock": "^0.7.5",
    "postcss": "^8.4.32",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "sass": "^1.70.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^4.9.5"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings=1000",
      "prettier -w"
    ],
    "**/*.{json,css,scss,md,webmanifest}": [
      "prettier -w"
    ]
  },
  "packageManager": "pnpm@9.13.0+sha1.f149ab27b27e712ac9a58ce1717c307364ccdca8"
